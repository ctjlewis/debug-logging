{
  "name": "debug-logging",
  "author": {
    "name": "C. Lewis",
    "email": "ctj.lewis@icloud.com"
  },
  "type": "module",
  "version": "4.0.2",
  "description": "Simple debug logging.",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "devDependencies": {
    "@tsmodule/tsmodule": "^40.0.11",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "ava": "^4.3.1",
    "eslint": "^8.21.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "dev": "tsmodule dev",
    "build": "tsmodule build",
    "test": "ava",
    "pretest": "tsmodule build --runtime-only",
    "prepublishOnly": "yarn build && yarn test",
    "lint": "eslint src --fix"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "console": "^0.7.2",
    "node-fetch": "^3.2.0",
    "socket.io": "^4.4.0"
  },
  "ava": {
    "timeout": "10m",
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@tsmodule/tsmodule"
    ]
  },
  "platform": "node"
}
